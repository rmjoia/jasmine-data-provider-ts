{
  "name": "jasmine-data-provider-ts",
  "version": "1.5.0",
  "description": "jasmine parameter test using generics based on JP Castro's Jasmine Data Provider",
  "main": "./dist/jasmine-data-provider.js",
  "types": "./dist/jasmine-data-provider.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "format": "prettier --write \"lib/**/*.ts\"",
    "lint": "eslint lib/**/*.ts tests/**/*.ts",
    "test": "tsx ./node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "prepare": "npm run build",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rmjoia/jasmine-data-provider-ts"
  },
  "keywords": [
    "typescript",
    "Jasmine",
    "data",
    "parameter"
  ],
  "author": "Ricardo Melo Joia",
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "^5.1.4",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "jasmine": "^5.5.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {}
}
